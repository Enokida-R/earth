{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/public/static/$1"
      },
      {
        "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico))",
        "dest": "/public/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/.*",
        "dest": "server.js"
      }
    ]
  }
  